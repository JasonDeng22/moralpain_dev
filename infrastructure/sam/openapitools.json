{
    "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
        "version": "5.4.0",
        "generators": {
            "v1.0.0": {
                "generatorName": "html2",
                "output": "#{cwd}/docs/#{name}",
                "glob": "infrastructure/sam/api.yaml",
                "additionalProperties": {
                    "appName": "moraldistress",
                    "appDescription": "API for the moraldistress project.",
                    "infoUrl": "https://github.com/kevinsullivan/moralpain_dev",
                    "licenseUrl": "https://github.com/kevinsullivan/moralpain_dev/blob/main/COPYRIGHT",
                    "withInterfaces": true
                }
            }
        }
    }
}
